{% extends '::backend.html.twig' %}

{% block body %}
<h3>Alumnos Proyectos list</h3>
<div class="table-container">
    <table class="width-100 table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>No Control</th>
                <th>Alumno</th>
                <th>Proyecto</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>

        {% for entity in alumnosenproyectos %}
            <tr>
                <td><a href="{{ path('alumnosproyectos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.idAlumno.noControl }}</td>
                <td>{{ entity.idAlumno.nombre ~ " " ~ entity.idAlumno.apellidoPaterno  ~ " " ~ entity.idAlumno.apellidoMaterno }}</td>
                <td>{{ entity.idProyecto.nombreCorto }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('alumnosproyectos_show', { 'id': entity.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('alumnosproyectos_edit', { 'id': entity.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>

    <ul>
        <li>
            <a href="{{ path('alumnosproyectos_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
{% endblock %}

{% block lateral %}
    <ul>
        <li><a href="{{ path('alumnos') }}">Alumnos</a></li>
        <li><a href="{{ path('proyectos') }}">Proyectos</a></li>
    </ul>
{% endblock %}
