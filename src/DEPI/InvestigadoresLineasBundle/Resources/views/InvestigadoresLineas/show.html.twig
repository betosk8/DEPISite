{% extends '::backend.html.twig' %}

{% block body %}
<div class="unit-100">
    <h1>InvestigadoresLineas</h1>

    <table class="pure-table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Rol</th>
                <td>{{ entity.rol }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('investigadoreslineas') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('investigadoreslineas_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
</div>
{% endblock %}